{
  "name": "redux-saga-modal",
  "version": "0.3.0",
  "description": "A wrapper to manage modals within redux-saga",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/redux-saga-modal.min.js",
  "scripts": {
    "clean": "rimraf es lib dist",
    "flow": "flow",
    "lint": "eslint 'src/**'",
    "build:commonjs": "cross-env babel src --out-dir lib",
    "build:es": "cross-env babel src --out-dir es",
    "build:umd": "webpack --mode development --display-cached --display-cached-assets -o dist/redux-saga-modal.js",
    "build:umd:min": "webpack --mode production -o dist/redux-saga-modal.min.js",
    "build": "npm-run-all clean -p build:commonjs build:umd build:umd:min",
    "build:watch": "nodemon --watch src --exec 'yarn run build:es'",
    "prepublish": "yarn run build",
    "release": "release-it --verbose --github.release"
  },
  "lint-staged": {
    "*.{js, json, lock}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "redux-saga",
    "modal",
    "redux"
  ],
  "author": "Karim Aziev <karim.aziev@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/KarimAziev/redux-saga-modal#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/KarimAziev/redux-saga-modal"
  },
  "bugs": {
    "url": "https://github.com/KarimAziev/redux-saga-modal/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "babel-eslint": "8.2.5",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "^1.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^4.1.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-react-app": "^3.0.0",
    "flow-bin": "^0.91.0",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.14.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-saga": ">=0.15.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "hoist-non-react-statics": "^2.0.0"
  },
  "peerDependencies": {
    "@babel/runtime": ">=7.0.0",
    "react": "^0.14.0 || >=15.0.0",
    "react-redux": "^4.0.0 || ^5.0.0",
    "redux": "^3.0.0 || ^4.0.0",
    "redux-saga": ">=0.15.0"
  },
  "testURL": "http://localhost"
}
