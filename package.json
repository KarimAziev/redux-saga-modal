{
  "name": "redux-saga-modal",
  "version": "1.3.2",
  "description": "A wrapper to manage modals within redux-saga",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib es",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\" index.d.ts \"**/*.md\"",
    "build": "webpack --mode production --config webpack.config.js",
    "build:dev": "yarn install --force && yarn build && rm -rf node_modules/react node_modules/react-redux",
    "release": "release-it",
    "lint": "eslint src",
    "prebuild": "npm-run-all -s clean",
    "prepublishOnly": "npm-run-all -s build",
    "precommit": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "npmName": "redux-saga-modal",
  "keywords": [
    "redux-saga",
    "modal",
    "redux",
    "dialog"
  ],
  "author": "Karim Aziev <karim.aziev@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/KarimAziev/redux-saga-modal#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/KarimAziev/redux-saga-modal"
  },
  "bugs": {
    "url": "https://github.com/KarimAziev/redux-saga-modal/issues"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^28.1.1",
    "@types/react": "18.2.6",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^5.59.7",
    "codecov": "^3.8.2",
    "cross-env": "^5.2.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "*",
    "react-redux": "8.0.2",
    "redux": "*",
    "redux-saga": "*",
    "rimraf": "^2.6.2",
    "ts-jest": "^28.0.5",
    "ts-loader": "^8.3.0",
    "typedoc": "^0.22.17",
    "typescript": "4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-redux": "*",
    "redux": "*",
    "redux-saga": "*"
  },
  "testURL": "http://localhost"
}
