{
  "name": "redux-saga-modal",
  "version": "0.4.15",
  "description": "A wrapper to manage modals within redux-saga",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": ["lib"],
  "scripts": {
    "clean": "rimraf lib es",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\" index.d.ts \"**/*.md\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.{js,ts}\" index.d.ts \"**/*.md\"",
    "build:local": "yarn build && yarn pack",
    "build": "cross-env NODE_ENV=production webpack --mode production --config webpack.config.js",
    "build:ts": "npm run build:lib && npm run build:es",
    "build:rollup": "cross-env NODE_ENV=production && rollup -c",
    "build:lib": "tsc",
    "start": "tsc --module es6 --outDir es --watch",
    "build:es": "tsc --module es6 --outDir es",
    "lint": "eslint src",
    "prebuild": "npm-run-all -s clean",
    "prepublishOnly": "npm-run-all -s build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": ["prettier-eslint --write", "git add"]
  },
  "npmName": "redux-saga-modal",
  "keywords": ["redux-saga", "modal", "redux", "dialog"],
  "author": "Karim Aziev <karim.aziev@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/KarimAziev/redux-saga-modal#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/KarimAziev/redux-saga-modal"
  },
  "bugs": {
    "url": "https://github.com/KarimAziev/redux-saga-modal/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.3.1",
    "@redux-saga/testing-utils": "^1.0.2",
    "@tsconfig/recommended": "^1.0.1",
    "@types/react": "^16.9.56",
    "@types/react-redux": "^7.1.11",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "^1.0.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-typescript": "^0.14.0",
    "invariant": "^2.2.4",
    "lint-staged": "^7.2.2",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "react": "*",
    "react-redux": "*",
    "redux": "*",
    "redux-saga": "*",
    "rimraf": "^2.6.2",
    "rollup": "^2.48.0",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "sinon": "^7.3.2",
    "ts-loader": "^8.0.3",
    "typescript": "4.2.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "@redux-saga/is": "^1.0.2",
    "hoist-non-react-statics": "^3.3.2",
    "prop-types": "^15.7.2",
    "webpack-node-externals": "^3.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-redux": "*",
    "redux": "*",
    "redux-saga": "*"
  },
  "testURL": "http://localhost"
}
